{
  "name": "wp-theme-wf",
  "version": "3.1.0",
  "author": "Chris Stielper",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "WordPress",
    "Theme"
  ],
  "browserslist": [
    "last 3 versions"
  ],
  "devDependencies": {
    "@wordpress/babel-preset-default": "^6.6.1",
    "@wordpress/scripts": "^22.2.1",
    "browser-sync": "^2.27.9",
    "dir-archiver": "^1.2.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-dart-sass": "^1.0.2",
    "gulp-newer": "^1.4.0",
    "gulp-notify": "^4.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify-es": "^3.0.0",
    "rtlcss": "^3.5.0"
  },
  "rtlcssConfig": {
    "options": {
      "autoRename": false,
      "autoRenameStrict": false,
      "blacklist": {},
      "clean": true,
      "greedy": false,
      "processUrls": false,
      "stringMap": []
    },
    "plugins": [],
    "map": false
  },
  "scripts": {
    "lint:scss": "wp-scripts lint-style 'sass/**/*.scss'",
    "lint:editor-scss": "wp-scripts lint-style 'editor-styles/**/*.scss'",
    "lint:acf-blocks-scss": "wp-scripts lint-style 'acf-blocks/**/*.scss'",
    "lint:js": "wp-scripts lint-js 'js/*.js'",
    "lint:acf-blocks-js": "wp-scripts lint-js 'acf-blocks/**/*.js'",
    "bundle": "dir-archiver --src . --dest ../_s.zip --exclude .DS_Store .stylelintrc.json .eslintrc .git .gitattributes .github .gitignore README.md composer.json composer.lock node_modules vendor package-lock.json package.json .travis.yml phpcs.xml.dist sass style.css.map"
  }
}
