{
	"env": {
		"browser": true,
		"es6": true,
		"jquery": true
	},
	"globals": {
		"wp": true,
		"google": true
	},
	"extends": "eslint:recommended",
	"rules": {
		"no-console": "warn",
		"indent": ["error", "tab"],
		"quotes": [2, "single"],
		"semi": [2, "always"],
		"comma-dangle": ["error", "never"]
	}
}
